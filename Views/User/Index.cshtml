@{
}
@model  FundingApplication.Models.User
<h3>hi @Model.Name </h3>
<h3>Salary Board</h3>
<br />
<table>
    <tr>
        <th>year</th>
        <th>Month</th>
        <th>Total</th>
    </tr>
    @{
        var salary = Model.salaries.ToArray();
    }
    @foreach (var item in @salary)
    {
        <tr>
            <td>@item.year</td>
            <td>@item.monthSalaries.Select(x=>x.month)</td>
            <td>@item.monthSalaries.Select(x=>x.Salary)</td>
        </tr>

    }
</table>